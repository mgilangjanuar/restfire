{
  "name": "restfire",
  "description": "The Web-based Test and Design API Platform",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "repository": {
    "url": "https://github.com/mgilangjanuar/restfire"
  },
  "author": {
    "name": "M Gilang Januar",
    "email": "mgilangjanuar@gmail.com",
    "url": "https://mgilangjanuar.com"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "18.0.0",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "ace-builds": "^1.4.12",
    "antd": "4.10.0",
    "axios": ">=0.21.1",
    "cors": "^2.8.5",
    "electron-log": "^4.2.4",
    "electron-updater": "^4.3.5",
    "filesize": "^6.1.0",
    "form-data": "^3.0.0",
    "immutability-helper": "^3.1.1",
    "parse-curl": "^0.2.6",
    "qs": "^6.9.4",
    "query-string": "^6.13.5",
    "react": "17.0.2",
    "react-ace": "^9.1.4",
    "react-confetti": "^6.0.0",
    "react-css-theme-switcher": "^0.1.6",
    "react-dom": "17.0.2",
    "react-hotkeys": "^2.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1",
    "react-use": "^15.3.4",
    "shortid": "^2.2.15",
    "typescript": "^5.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-prebuild": "react-scripts build && mkdir buildelectron && cp -r build buildelectron && cp -r server buildelectron && rm -rf build && cp electron.js public/ && cp electron.js buildelectron/ && mv buildelectron build",
    "electron-build": "npm run electron-prebuild && electron-builder"
  },
  "build": {
    "appId": "com.mgilangjanuar.restfire",
    "productName": "RestFire Studio (Beta)",
    "win": {
      "publish": [
        "github"
      ]
    },
    "mac": {
      "publish": [
        "github"
      ],
      "extendInfo": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "linux": {
      "publish": [
        "github"
      ]
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/antd": "^1.0.0",
    "@types/axios": "^0.14.0",
    "@types/electron": "^1.6.10",
    "@types/filesize": "^5.0.0",
    "@types/form-data": "^2.5.0",
    "@types/immutability-helper": "^2.6.3",
    "@types/qs": "^6.9.5",
    "@types/query-string": "^6.3.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/shortid": "^0.0.29",
    "electron": "^23.1.4",
    "electron-builder": "^23.6.0"
  }
}
